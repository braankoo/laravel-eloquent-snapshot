{
    "name": "braankoo/eloquent-snapshot",
    "description": "Laravel Eloquent Snapshot is a lightweight package that enables automatic or manual snapshots of Eloquent model states. Ideal for audit trails, change tracking, or rollback functionality, this package stores historical records of model data in a flexible and efficient way.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Branko Dragovic"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/console": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Braankoo\\EloquentSnapshot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Braankoo\\EloquentSnapshot\\EloquentSnapshotServiceProvider"
            ],
            "aliases": {
                "LaravelEloquentSnapshot": "Braankoo\\EloquentSnapshot\\EloquentSnapshotFacade"
            }
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "laravel/pint": "dev-main"
    }
}
